{
  "id" : "${artifactId}-${version}",
  "name" : "Hello World",
  "provides" : [ {
    "id" : "hello",
    "version" : "1.1",
      "handlers" : [ {
        "methods" : [ "GET", "POST" ],
        "pathPattern" : "/hello"
      },{
        "methods" : [ "GET" ],
        "pathPattern" : "/fileChunked",
        "type": "request-response"
      },{
        "methods" : [ "GET" ],
        "pathPattern" : "/fileChunked-1-0",
        "type": "request-response-1.0"
      },{
        "methods" : [ "GET" ],
        "pathPattern" : "/fileUnchunked",
        "type": "request-response"
      },{
        "methods" : [ "GET" ],
        "pathPattern" : "/fileUnchunked-1-0",
        "type": "request-response-1.0"
      } ]
  } ],
  "launchDescriptor" : {
    "dockerImage" : "folio-hello-module"
  }
}
